<div class='wrapper'>
  <div class='left-side'>
    <div class='app-title'>タスク管理アプリ</div>
  </div>
  <div class='right-side'>
    <div class=new-task-btn>
      <%= link_to "新規投稿", new_task_path %>
    </div>
  </div>
</div>
<div class='content-wrapper'>
  <div class='side-menu'>
    <div class='tasks' >
    <% @tasks.each do |task| %>
      <div class='task'>
        <%= task.title%>
      </div>
    <% end %>
    </div>
  </div>
  <div class='main-task'>
  </div>
</div>